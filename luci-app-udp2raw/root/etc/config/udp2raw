config general
	option enabled '0'
	option server 'nil'
	option daemon_user 'root'

config servers
	option alias 'MyServer'
	option server_addr '192.168.1.100'
	option server_port '4096'
	option local_addr '0.0.0.0'
	option local_port '7777'
	option raw_mode 'faketcp'
	option key 'mypassword'
	option cipher_mode 'aes128cbc'
	option auth_mode 'md5'
	option auto_rule '1'
	option keep_rule '0'
	option seq_mode '1'
	option lower_level 'no'
	option retry_on_error '0'
	list extra_args ''

config clients
	option alias 'MyClient'
	option server_addr 'example.com'
	option server_port '4096'
	option local_addr '127.0.0.1'
	option local_port '7777'
	option raw_mode 'faketcp'
	option key 'mypassword'
	option cipher_mode 'aes128cbc'
	option auth_mode 'md5'
	option auto_rule '1'
	option keep_rule '0'
	option seq_mode '1'
	option lower_level 'no'
	option retry_on_error '0'
	list extra_args ''