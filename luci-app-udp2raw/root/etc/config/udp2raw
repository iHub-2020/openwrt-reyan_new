# Project: LuCI Support for udp2raw
# Description: Web interface for udp2raw tunnel configuration
# Version: 1.0.0
# Last Updated: 2026-01-09
# Depends: udp2raw (>= 20230206.0)

config global 'global'
    option enable '0'

# 客户端模式：OpenWrt 作为客户端，连接远程 VPS
config client
    option enable '0'
    option server_addr '44.55.66.77'
    option server_port '4096'
    option local_addr '127.0.0.1'
    option local_port '3333'
    option password 'password'
    option raw_mode 'faketcp'
    option cipher_mode 'aes128cbc'
    option auth_mode 'hmac_sha1'
    option fix_gro '1'
    option keep_rule '1'
    option auto_rule '0'
    option seq_mode '3'

# 服务端模式：OpenWrt 作为服务端，等待外部连接
config server
    option enable '0'
    option local_addr '0.0.0.0'
    option local_port '4096'
    option remote_addr '0.0.0.0'
    option remote_port '7777'
    option password 'password'
    option raw_mode 'faketcp'
    option cipher_mode 'aes128cbc'
    option auth_mode 'hmac_sha1'
    option fix_gro '1'
    option keep_rule '1'
    option auto_rule '0'
    option seq_mode '3'
