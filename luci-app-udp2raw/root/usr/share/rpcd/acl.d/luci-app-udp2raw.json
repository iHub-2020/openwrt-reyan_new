{
	"luci-app-udp2raw": {
		"description": "Grant access to udp2raw configuration",
		"read": {
			"uci": ["udp2raw"],
			"ubus": {
				"service": ["list", "get_data"],
				"file": ["read", "stat"]
			},
			"file": {
				"/usr/bin/udp2raw": ["exec"],
				"/var/run/udp2raw-*.pid": ["read"],
				"/tmp/udp2raw.*.log": ["read"]
			}
		},
		"write": {
			"uci": ["udp2raw"],
			"ubus": {
				"service": ["set", "delete", "event"]
			},
			"file": {
				"/etc/init.d/udp2raw": ["exec"]
			}
		}
	}
}
