{
	"luci-app-udp2raw": {
		"description": "Grant access to udp2raw tunnel configuration",
		"read": {
			"uci": ["udp2raw"],
			"ubus": {
				"service": ["list", "get_data"],
				"file": ["read", "stat", "exec"]
			},
			"file": {
				"/usr/bin/udp2raw": ["exec"],
				"/var/run/udp2raw-*.pid": ["read"],
				"/usr/bin/logread": ["exec"],
				"/usr/sbin/iptables": ["exec"],
				"/usr/sbin/ip6tables": ["exec"]
			}
		},
		"write": {
			"uci": ["udp2raw"],
			"ubus": {
				"service": ["set", "delete", "event"]
			},
			"file": {
				"/etc/init.d/udp2raw": ["exec"]
			}
		}
	}
}